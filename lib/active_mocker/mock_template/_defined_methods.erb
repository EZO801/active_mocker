<% instance_methods.each do |method| -%>
  def <%= method.name %><%= "(#{method.arguments})" unless method.arguments.blank? %>
    <%= method.body %>
  end
<% end -%>
module ClassMethods
<% class_methods.each do |method| -%>
    def <%= method.name %><%= "(#{method.arguments})" unless method.arguments.blank? %>
    <%= method.body %>
  end
<% end -%>
end

extend ClassMethods
